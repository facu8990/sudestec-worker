{
	"compilerOptions": {
		"target": "ESNext",
		"module": "ESNext",
		"moduleResolution": "node",
		"esModuleInterop": true,
		"strict": true,
		"lib": [
			"esnext"
		],
		"types": [
			"@cloudflare/workers-types"
		],
		"plugins": [
			{
				"name": "typescript-lit-html-plugin",
				"format": {
					"enabled": true
				},
				"tags": [
					"html",
					"raw"
				]
			}
		]
	},
}